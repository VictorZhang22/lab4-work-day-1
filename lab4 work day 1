def fineMultiply():
    x = 0
    for number in range(1, 1000):
        if number % 3 == 0 or number % 5 == 0:
            x = x + number
    print(x)

def calculate():
    sumOfEven = 0
    x = 1
    y = 2
    while y <= 4000000:
        if y % 2 == 0:
            sumOfEven += y
        y += x
        x = y - x
    return sumOfEven

